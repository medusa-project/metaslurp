<table class="table">
  <thead>
    <tr>
      <th style="min-width: 20%">Name</th>
      <th>Key</th>
    </tr>
  </thead>
  <tbody>
    <% @content_services.each do |service| %>
      <tr>
        <td><%= link_to service.name, admin_content_service_path(service) %></td>
        <td><code><%= service.key %></code></td>
      </tr>
    <% end %>
  </tbody>
</table>
